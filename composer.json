{
    "name": "ebsp/resting",
    "description": "Simple REST library for Laravel",
    "require": {
        "laravel/framework": "^5.7 || ^6.0 || ^7.0",
        "codeception/assert-throws": "^1.0"
    },
    "authors": [
        {
            "name": "Emil BÃ¼chler Seier Petersen",
            "email": "emil@ebsp.dk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Seier\\Resting\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Seier\\Resting\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seier\\Resting\\Support\\RestingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "9.0.0",
        "orchestra/testbench": "^4.0"
    },
    "scripts": {
    	"coverage": "phpunit --coverage-html ./docs/coverage"
    }
}
